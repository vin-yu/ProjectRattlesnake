@{
    ViewData["Title"] = "Transcripts";
}
<div class="page-title">
    <div class="title_left">
        <h3>My Transcripts</h3>
    </div>
    <div class="title_right">
        <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
            <div class="input-group">
                <input type="text" class="form-control" placeholder="Search for...">
                <span class="input-group-btn">
                    <button class="btn btn-default" type="button">Go!</button>
                </span>
            </div>
        </div>
    </div>
</div>
<div class="clearfix"></div>


<div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
    @{
        for (var i = 0; i < ViewBag.results.Length; i++) {
            var meeting = ViewBag.results[i].Item1;
            var recording = ViewBag.results[i].Item2;
            var transcript = ViewBag.results[i].Item3;
            var heading = "heading" + i;
            var collapse = "collapse" + i;
            var collapse2 = "#collapse" + i;
            var meetingTabName = "meeting" + i;
            var meetingTabName2 = "#meeting" + i;
            var recordingTabName = "recording" + i;
            var recordingTabName2 = "#recording" + i;
            var transcriptTabName = "transcript" + i;
            var transcriptTabName2 = "#transcript" + i;

            {
                <div class="panel panel-default">
                    <div class="panel-heading" role="tab" id=@heading>
                        <h4 class="panel-title">
                            <a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion" href=@collapse2>
                                @meeting
                            </a>
                        </h4>
                    </div>
                    <div id=@collapse class="panel-collapse collapse" role="tabpanel" aria-labelledby=@heading>
                        <div class="panel-body">
                            <div>
                                <ul class="nav nav-tabs" role="tablist">
                                    <li role="presentation" class="active"><a href=@meetingTabName2 role="tab" data-toggle="tab">Meeting Information</a></li>
                                    <li role="presentation"><a href=@recordingTabName2 role="tab" data-toggle="tab">Recording</a></li>
                                    <li role="presentation"><a href=@transcriptTabName2 role="tab" data-toggle="tab">Transcript</a></li>
                                </ul>
                                <!-- Tab panes -->
                                <div class="tab-content">
                                    <div role="tabpanel" class="tab-pane active" id=@meetingTabName>@meeting</div>
                                    <div role="tabpanel" class="tab-pane" id=@recordingTabName>@recording</div>
                                    <div role="tabpanel" class="tab-pane" id=@transcriptTabName>@transcript</div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            }
        }
    }
  

<div class="col-md-6 col-sm-6 col-xs-12">
    <div class="x_panel">
        <div class="x_content">

            <div class="" role="tabpanel" data-example-id="togglable-tabs">
                <ul id="myTab" class="nav nav-tabs bar_tabs" role="tablist">
                    <li role="presentation" class="active">
                        <a href="#tab_content1" id="home-tab" role="tab" data-toggle="tab" aria-expanded="true" style="color:#071033;">Meeting Information</a>
                    </li>
                    <li role="presentation" class="">
                        <a href="#tab_content2" role="tab" id="profile-tab" data-toggle="tab" aria-expanded="false"  style="color:#071033;">Recording</a>
                    </li>
                    <li role="presentation" class="">
                        <a href="#tab_content3" role="tab" id="profile-tab2" data-toggle="tab" aria-expanded="false" style="color:#071033;">Transcript</a>
                    </li>
                </ul>
                <div id="myTabContent" class="tab-content">
                    <div role="tabpanel" class="tab-pane fade active in" id="tab_content1" aria-labelledby="home-tab" runat="server">
                        <p>
